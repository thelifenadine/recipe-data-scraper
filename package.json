{
  "name": "recipe-data-scraper",
  "version": "1.4.1",
  "description": "web scraper for recipe data",
  "repository": "github:thelifenadine/recipe-data-scraper",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "webpack",
    "build:ts": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "build-watch": "webpack -w",
    "test": "node --test --require ts-node/register 'src/**/*.test.ts'",
    "test:legacy": "cross-env NODE_ENV=test nyc mocha './src/**/*.test.js' --exit",
    "lint": "eslint './src/**/*.{js,ts}'",
    "print-recipe-tester": "cross-env LOGGING_ENABLED=true ts-node ./scripts/printRecipeTester.ts",
    "test-url": "cross-env LOGGING_ENABLED=true ts-node ./scripts/urlTesterCLI.ts"
  },
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "7.27.4",
    "axios": "1.10.0",
    "cheerio": "1.1.0",
    "iso8601-duration": "2.1.2",
    "microdata-node": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.27.1",
    "@babel/plugin-transform-runtime": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.27.1",
    "@types/cheerio": "0.22.35",
    "@types/node": "^18.19.54",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-babel": "5.3.1",
    "readline-sync": "1.4.10",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "webpack": "5.99.9",
    "webpack-cli": "4.9.1"
  }
}
