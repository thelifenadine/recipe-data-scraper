{
  "name": "recipe-data-scraper",
  "version": "1.4.1",
  "description": "web scraper for recipe data",
  "repository": "github:thelifenadine/recipe-data-scraper",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "webpack",
    "build:ts": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "build-watch": "webpack -w",
    "test-old": "mocha './src/**/*.test.js' --require @babel/register --reporter spec || exit 0",
    "lint": "eslint './src/**/*.{js,ts}'",
    "test": "cross-env NODE_ENV=test nyc mocha './src/**/*.test.js' --exit",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "print-recipe-tester": "cross-env LOGGING_ENABLED=true babel-node --extensions .js,.ts ./scripts/printRecipeTester.ts",
    "test-url": "cross-env LOGGING_ENABLED=true babel-node --extensions .js,.ts ./scripts/urlTesterCLI.ts"
  },
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "7.27.4",
    "axios": "1.10.0",
    "cheerio": "1.1.0",
    "iso8601-duration": "2.1.2",
    "microdata-node": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.27.1",
    "@babel/plugin-transform-runtime": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.27.1",
    "@types/chai": "4.3.19",
    "@types/cheerio": "0.22.35",
    "@types/mocha": "10.0.8",
    "@types/node": "18.19.54",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-loader": "8.2.3",
    "babel-plugin-istanbul": "6.1.1",
    "chai": "4.3.4",
    "clean-webpack-plugin": "4.0.0",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-babel": "5.3.1",
    "istanbul": "0.4.5",
    "mocha": "9.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "readline-sync": "1.4.10",
    "sinon": "12.0.1",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.1",
    "typescript": "5.6.3",
    "webpack": "5.99.9",
    "webpack-cli": "4.9.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
