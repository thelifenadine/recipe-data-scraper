{
  "name": "recipe-data-scraper",
  "version": "2.0.0",
  "description": "web scraper for recipe data",
  "repository": "github:thelifenadine/recipe-data-scraper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack",
    "build:ts": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "build-watch": "webpack -w",
    "test": "node --test --require ts-node/register 'src/**/*.test.ts'",
    "test:coverage": "node --test --experimental-test-coverage --require ts-node/register 'src/**/*.test.ts'",
    "coverage": "node --test --experimental-test-coverage --require ts-node/register 'src/**/*.test.ts'",
    "lint": "eslint './src/**/*.{js,ts}'",
    "print-recipe-tester": "cross-env LOGGING_ENABLED=true ts-node ./scripts/printRecipeTester.ts",
    "test-url": "cross-env LOGGING_ENABLED=true ts-node ./scripts/urlTesterCLI.ts"
  },
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "iso8601-duration": "2.1.2",
    "microdata-node": "2.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.35",
    "@types/node": "^18.19.121",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "readline-sync": "1.4.10",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0",
    "webpack-cli": "4.9.1"
  }
}
